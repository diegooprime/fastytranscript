{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fetch-youtube-transcript",
  "version": "3.0.0",
  "title": "FastyTranscript",
  "description": "Lightning-fast YouTube transcript fetcher with auto-detection, Markdown output, and timestamp support",
  "icon": "extension_icon.png",
  "author": "apoorv_khandelwal",
  "contributors": [
    "pernielsentikaer",
    "ridemountainpig",
    "diegoships101"
  ],
  "categories": [
    "Media",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "fetch-youtube-transcript",
      "title": "FastyTranscript",
      "description": "Instantly fetch YouTube transcript from clipboard/browser",
      "mode": "view"
    }
  ],
  "preferences": [],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^2.2.0",
    "youtubei.js": "^16.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "overrides": {
    "cross-spawn": "^7.0.3",
    "got": "^13.0.0",
    "electron": "^32.0.0"
  },
  "platforms": [
    "macOS"
  ]
}
